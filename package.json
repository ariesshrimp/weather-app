{
  "name": "Weather-and-Maps",
  "version": "1.0.0",
  "description": "Get a map and a weather forecast for anywhere in the world.",
  "dependencies": {
    "forecast-io": "^1.0.3",
    "google-maps-api": "^2.0.1",
    "react": "^15.3.1",
    "react-d3": "^0.4.0",
    "react-dom": "^15.3.1",
    "react-google-maps": "^7.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "aws-sdk": "^2.6.3",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-istanbul": "^0.12.2",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.16.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "codecov": "^2.2.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "firebase-tools": "^3.0.7",
    "html-webpack-plugin": "^2.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jsdom": "^11.1.0",
    "json-loader": "^0.5.4",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.5",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.3.1",
    "react-shallow-testutils": "^3.0.0",
    "react-test-renderer": "^15.3.1",
    "sass-loader": "^6.0.6",
    "source-map-support": "^0.4.2",
    "style-loader": "^0.18.2",
    "svg-react-loader": "^0.4.4",
    "testcheck": "^0.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "scripts": {
    "test": "jest && codecov",
    "dev": "babel-node node_modules/.bin/webpack-dev-server",
    "build": "rm -rf public && babel-node node_modules/.bin/webpack",
    "deploy": "firebase deploy"
  },
  "jest": {
    "testRegex": "__tests__.+test.js$",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleNameMapper": {
      "moduleFileExtensions": [
        "js"
      ],
      "moduleDirectories": [
        "node_modules"
      ],
      "^.+\\.(css|scss)$": "<rootDir>/__tests__/mocks/styleMock.js",
      "^.+\\.(svg)$": "<rootDir>/__tests__/mocks/fileMock.js"
    }
  },
  "author": "Joe Fraley",
  "license": "MIT"
}
